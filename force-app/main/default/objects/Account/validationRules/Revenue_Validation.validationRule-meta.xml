<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Revenue_Validation</fullName>
    <active>false</active>
    <errorConditionFormula>IF( AnnualRevenue &gt;= (20 *  $CustomMetadata.Support_Tier__mdt.Gold.Minimum_Spending__c), 
TRUE,
FALSE)</errorConditionFormula>
    <errorMessage>Annual Revenue is not high enough</errorMessage>
</ValidationRule>
